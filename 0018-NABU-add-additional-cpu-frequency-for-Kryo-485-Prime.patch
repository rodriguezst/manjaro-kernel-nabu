From c2f969e276098ce9a32c63df1adfd08870914386 Mon Sep 17 00:00:00 2001
From: maverick <maverick_jia@sina.com>
Date: Sat, 4 Nov 2023 17:26:44 +0800
Subject: [PATCH 18/43] NABU: add additional cpu frequency for Kryo 485 Prime

---
 arch/arm64/boot/dts/qcom/sm8150-xiaomi-nabu-maverick.dts | 8 ++++++++
 1 file changed, 8 insertions(+)

diff --git a/arch/arm64/boot/dts/qcom/sm8150-xiaomi-nabu-maverick.dts b/arch/arm64/boot/dts/qcom/sm8150-xiaomi-nabu-maverick.dts
index 797381aef..c931d9887 100644
--- a/arch/arm64/boot/dts/qcom/sm8150-xiaomi-nabu-maverick.dts
+++ b/arch/arm64/boot/dts/qcom/sm8150-xiaomi-nabu-maverick.dts
@@ -435,6 +435,14 @@ vreg_l11c_3p3: ldo11 {
 	/* PM8009 is not present on these boards, even if downstream sources suggest so. */
 };
 
+//Kryo 485 Prime additional cpu frequency
+&cpu7_opp_table {
+	cpu7_opp21: opp-2956800000 {
+		opp-hz = /bits/ 64 <2956800000>;
+		opp-peak-kBps = <8368000 51609600>;
+	};
+};
+
 &gmu {
 	status = "okay";
 };
