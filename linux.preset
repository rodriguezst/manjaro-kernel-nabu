# mkinitcpio preset file for the '%PKGBASE%' package

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="%KERNVER%"

PRESETS=('default')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-linux.img"
default_uki="/boot/efi/EFI/manjaro/linux-6.12.efi"
default_options="--kernelimage /boot/vmlinux-%KERNVER% --ukiconfig /etc/kernel/uki.conf --cmdline /etc/kernel/cmdline"
